page {
  height: 100%;
  width: 100%;
  background-color: rgb(178, 47, 55);
  color: rgb(255, 212, 181);
}
.container {
  width: 100%;
  height: 100%;
}
.model {
  position: absolute;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  margin: 0 auto;
  /* background-color: rgb(178, 47, 55); */
}
.model_show {
  animation: slide-top 0.5s ease-out both;
}
.model_hide {
  animation: slide-bottom 0.5s ease-out both;
}
.model .bg {
  position: absolute;
  width: 100%;
  height: 100%;
  /* z-index: -1; */
}
.model .content {
  width: 100%;
  height: 100%;
  position: relative;
  text-align: center;
}


