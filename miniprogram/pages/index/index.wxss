.model-pic{}
.model-pic .pic{
  position: absolute;
  left: 0;
  top: 0;
  width: 100vw;
  height: 100vh;
  animation-timing-function: ease-in;
  animation-duration: 2s;
}
.model-pic.active .pic.left-in{
  animation-name: slide-from-left;
}
.model-pic.active .pic.right-in{
  animation-name: slide-from-right;
}
.model-pic.active .pic.top-in{
  animation-name: slide-from-top;
}
.model-pic.active .pic.bottom-in{
  animation-name: slide-from-bottom;
}
.model-pic.active .pic.fade-in{
  animation-name: fade-in;
}
.model-pic.active .pic.scale-in{
  animation-name: scale-in;
}
.model-pic.active .pic.delay-0_5s{
  animation-delay: 0.5s;
}
.model-pic.active .pic.delay-1s{
  animation-delay: 1s;
}
.model-pic.active .pic.delay-1_5s{
  animation-delay: 1.5s;
}
.model-pic.active .pic.delay-2s{
  animation-delay: 2s;
}
.model-pic.active .pic.delay-2_5s{
  animation-delay: 2.5s;
}
.model-pic.active .pic.delay-3s{
  animation-delay: 3s;
}

.model-pic.active .pic.duration-0_5s{
  animation-duration: 0.5s;
}
.model-pic.active .pic.duration-1s{
  animation-duration: 1s;
}
.model-pic.active .pic.duration-1_5s{
  animation-duration: 1.5s;
}
.model-pic.active .pic.duration-2s{
  animation-duration: 2s;
}
.model-pic.active .pic.duration-2_5s{
  animation-duration: 2.5s;
}
.model-pic.active .pic.duration-3s{
  animation-duration: 2s;
}

